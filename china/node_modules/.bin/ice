#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/china/china/node_modules/.pnpm/@ice+app@3.6.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/@ice/app/bin/node_modules:/mnt/c/china/china/node_modules/.pnpm/@ice+app@3.6.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/@ice/app/node_modules:/mnt/c/china/china/node_modules/.pnpm/@ice+app@3.6.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/@ice/node_modules:/mnt/c/china/china/node_modules/.pnpm/@ice+app@3.6.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules:/mnt/c/china/china/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/china/china/node_modules/.pnpm/@ice+app@3.6.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/@ice/app/bin/node_modules:/mnt/c/china/china/node_modules/.pnpm/@ice+app@3.6.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/@ice/app/node_modules:/mnt/c/china/china/node_modules/.pnpm/@ice+app@3.6.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/@ice/node_modules:/mnt/c/china/china/node_modules/.pnpm/@ice+app@3.6.1_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules:/mnt/c/china/china/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@ice/app/bin/ice-cli.mjs" "$@"
else
  exec node  "$basedir/../@ice/app/bin/ice-cli.mjs" "$@"
fi
